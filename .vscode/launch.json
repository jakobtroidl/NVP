{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.1.0",
    "configurations": [
        {
            "name": "Create 2D Dataset",
            "type": "python",
            "request": "launch",
            "module": "experiment_scripts.create_dataset",
            "justMyCode": true,
            "args": [
                "--name=minnen-denoised-small",
                "--path=https://storage.googleapis.com/h01-release/data/20210601/roi215/denoised",
                "--image_size=256",
                "--n_images=100",
                //"--train",
                "--anistropic_dim=2"
            ]
        },
        {
            "name": "Train Images",
            "type": "python",
            "request": "launch",
            "module": "experiment_scripts.train_images",
            "justMyCode": true,
            "args": [
                "--experiment_name=images-h01-denoised-small",
                "--dataset=data/minnen-denoised-2D-small/info.json",
                "--config=./config/config_cvr_s.json"
            ]
        },
        {
            "name": "Train Volume",
            "type": "python",
            "request": "launch",
            "module": "experiment_scripts.create_dataset",
            "justMyCode": true,
            "args": [
                "--name=./logs",
                "--experiment_name=h01-denoised-small",
                "--dataset=data/volumes/h01-denoised-small/info.json",
                "--config=./config/config_nvp_s.json"
            ]
        },
        {
            "name": "Eval Volume",
            "type": "python",
            "request": "launch",
            "module": "experiment_scripts.eval_volume",
            "justMyCode": true,
            "args": [
                "--logging_root=./logs",
                "--experiment_name=h01-denoised-small",
                "--dataset=data/volumes/h01-denoised-small/info.json",
                "--num_frames=128",
                "--config=./logs/h01-denoised-small/config.json",
                "--save",
                "--t_interp=4"
            ]
        }
    ]
}